<h1>関数これくしょん</h1>
<p>関娘をドックに集めましょう</p>

<p>
<%= link_to '新規メソッド追加', 'home/new' %>
<%= link_to '検索', 'home/search' %>
</p>
<table border="1">
<thead>
	<tr>
		<th>ID</th>
		<th>言語</th>
		<th>メソッド名</th>	
		<th>更新時間</th>
	</tr>
</thead>
<% @indexFunction.each do |index| %>
	<tbody>
	<tr>
		<td><%= index.id %></td>
		<td><%= index.languages %></td>
 		<td><%= link_to "index.names", { :action => "admin_result", :id => index.id },  :method => :post %>/</td> 
		<td><%= index.updated_at.strftime("%m月%d日 %H時%M分") %></td>
	</tr>
	</tbody>
<% end %>
</table>